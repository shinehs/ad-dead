{
    "name": "ad_dead",
    "version": "1.0",
    "manifest_version": 1,
    "description": "baidu_ad_dead",
    "permissions": [
        "http://*/"
    ],
    "icons": {
        "16": "ad-dead-16.png",
        "128": "ad-dead-128.png"
    },
    "browser_action": {
        "default_icon": "ad-dead-16.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["http://*/", "https://*/", "http://*/*", "https://*/*"],
        "js": ["js/jquery-1.10.1.min.js", "js/ad-dead.js"]
    }]
}